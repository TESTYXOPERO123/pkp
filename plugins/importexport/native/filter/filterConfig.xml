			class="lib.pkp.plugins.importexport.native.filter.NativeXmlReviewRoundFilter"
			isTemplate="0" />
		<filter
			inGroup="native-xml=>review-assignment"
			class="lib.pkp.plugins.importexport.native.filter.NativeXmlReviewAssignmentFilter"
			isTemplate="0" />
		<filter
			inGroup="query=>native-xml"
			class="lib.pkp.plugins.importexport.native.filter.QueryNativeXmlFilter"
			isTemplate="0" />
		<filter
			inGroup="note=>native-xml"
			class="lib.pkp.plugins.importexport.native.filter.NoteNativeXmlFilter"
			isTemplate="0" />
		<filter
			inGroup="native-xml=>query"
			class="lib.pkp.plugins.importexport.native.filter.NativeXmlQueryFilter"
			isTemplate="0" />
		<filter
			inGroup="native-xml=>note"
			class="lib.pkp.plugins.importexport.native.filter.NativeXmlNoteFilter"
			isTemplate="0" />
		<filter
			inGroup="review-form=>native-xml"
			class="lib.pkp.plugins.importexport.native.filter.ReviewFormNativeXmlFilter"
			isTemplate="0" />
		
		<filter
			inGroup="native-xml=>review-form"
			class="lib.pkp.plugins.importexport.native.filter.NativeXmlReviewFormFilter"
			isTemplate="0" />
		<filter
			inGroup="context=>native-xml"
			class="lib.pkp.plugins.importexport.native.filter.ContextNativeXmlFilter"
			isTemplate="0" />
	</filters>
</filterConfig>
