			symbolic="note=>native-xml"
			displayName="plugins.importexport.native.displayName"
			description="plugins.importexport.native.description"
			inputType="class::lib.pkp.classes.note.Note[]"
			outputType="xml::schema(lib/pkp/plugins/importexport/native/pkp-native.xsd)" />
