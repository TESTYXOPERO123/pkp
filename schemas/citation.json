			"multilingual": false,
			"apiSummary": true,
			"validation": [
				"nullable",
				"date_format:Y-m-d"
			]
		},
		"origtitle": {
			"type": "string",
			"description": "The title of the original work.",
			"multilingual": false,
			"apiSummary": true,
			"validation": [
				"nullable",
				"no_new_line"
			]
		},
		"origlanguage": {
			"type": "string",
			"description": "The language of the original work.",
			"multilingual": false,
			"apiSummary": true,
			"validation": [
				"regex:/^([A-Za-z]{2,4})(?<sc>[_-]([A-Za-z]{4,5}|[0-9]{4}))?([_-]([A-Za-z]{2}|[0-9]{3}))?(@[a-z]{2,30}(?&sc)?)?$/"
			]
		},
		"pubstate": {
			"type": "string",
			"description": "The state of this work, e.g. submitted.",
			"multilingual": false,
			"apiSummary": true,
			"validation": [
				"nullable",
				"string:'submitted','inpreparation','forthcoming','inpress','prepublished'"
			]
		},
		"eprinttype": {
			"type": "string",
			"description": "Eprint identifier type.",
			"multilingual": false,
			"apiSummary": true,
			"validation": [
				"nullable"
			]
		},
		"pagination": {
			"type": "string",
			"description": "The pagination of this work.",
			"multilingual": false,
			"apiSummary": true,
			"validation": [
				"nullable",
				"string:'page','column','section','paragraph','verse','line'"
			]
		},
		"eprintclass": {
			"type": "string",
			"description": "Additional information to an online publication.",
			"multilingual": false,
			"apiSummary": true,
			"validation": [
				"nullable"
			]
		}
